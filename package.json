    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "printWidth": 100,
          "tabWidth": 4,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "tests/api/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 4,
